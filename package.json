{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"cap-dev-android": "export MODE=dev && npm run build && npx cap sync && npx cap run android && npm run dev",
		"cap-dev-ios": "export MODE=dev && npm run build && npx cap sync && npm run dev",
		"cap-sync": "export MODE=prod npm run build && npx cap sync",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e"
	},
	"devDependencies": {
		"@capacitor/cli": "^7.0.1",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@iconify-json/mdi": "^1.2.3",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/vite": "^4.0.0",
		"@types/crypto-js": "^4.2.2",
		"eslint": "^9.18.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"unplugin-icons": "^22.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@capacitor/android": "^7.0.1",
		"@capacitor/app": "^7.0.0",
		"@capacitor/core": "^7.0.1",
		"@capacitor/ios": "^7.0.1",
		"@capacitor/preferences": "^7.0.0",
		"@capacitor/text-zoom": "^7.0.0",
		"@inlang/paraglide-sveltekit": "^0.15.5",
		"chart.js": "^4.4.7",
		"crypto-js": "^4.2.0",
		"dotenv": "^16.4.7"
	}
}
